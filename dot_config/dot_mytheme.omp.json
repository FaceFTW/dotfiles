{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"segments": [
				{
					"background": "#2f2f2f",
					"foreground": "#26C6DA",
					"leading_diamond": "\ue0c7\u2593",
					"properties": {
						"alpine": "\uf300",
						"arch": "\uf303",
						"centos": "\uf304",
						"debian": "\uf306",
						"elementary": "\uf309",
						"fedora": "\uf30a",
						"gentoo": "\uf30d",
						"linux": "\ue712",
						"macos": "\ue711",
						"manjaro": "\uf312",
						"mint": "\uf30f",
						"opensuse": "\uf314",
						"raspbian": "\uf315",
						"ubuntu": "\uf31c",
						"windows": "\ue70f"
					},
					"style": "diamond",
					"template": " {{ if .WSL }}\ue712 on {{ end }}{{ .Icon }}<#7a7a7a> \ue0b1</>",
					"type": "os"
				},
				{
					"type": "shell",
					"style": "diamond",
					"foreground": "#fafafa",
					"background": "#2f2f2f",
					"template": " {{ .Name }} -"
				},
				{
					"background": "#2f2f2f",
					"foreground": "#fafafa",
					"style": "diamond",
					"template": " {{ if ne .Env.POSH_SESSION_DEFAULT_USER .UserName }}<#77f5d6>{{ .UserName }}</><#7a7a7a>\uf1fa</>{{ end }}<#2EEFBF>{{ .HostName }}</><#7a7a7a> \ue0b1</>",
					"type": "session"
				},
				{
					"background": "#2f2f2f",
					"foreground": "#ffff66",
					"style": "diamond",
					"template": " \uf0e7<#7a7a7a> \ue0b1</>",
					"type": "root"
				},
				{
					"background": "#2f2f2f",
					"foreground": "#fafafa",
					"properties": {
						"folder_icon": "<#B5B2C2>\ue5fe </>",
						"style": "unique"
					},
					"style": "diamond",
					"template": "<#f2f200> \ue5ff </>{{ .Path }} ",
					"type": "path"
				},
				{
					"background": "#2f2f2f",
					"foreground": "#ffeb3b",
					"foreground_templates": [
						"{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb3b{{ end }}",
						"{{ if gt .Ahead 0 }}#2EC4B6{{ end }}",
						"{{ if gt .Behind 0 }}#8A4FFF{{ end }}"
					],
					"properties": {
						"fetch_stash_count": true,
						"fetch_status": true,
						"fetch_upstream_icon": true
					},
					"style": "diamond",
					"template": "<#7a7a7a>\ue0b1</> {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}<#E84855> \uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#2FDA4E> \uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
					"type": "git"
				},
				{
					"type": "firebase",
					"style": "diamond",
					"background": "#FFA000",
					"foreground": "#2f2f2f",
					"template": "<#7a7a7a>\ue0b1</> \ue657 "
				},
				{
					"type": "react",
					"style": "diamond",
					"foreground": "#81a1c1",
					"background": "#2f2f2f",
					"template": "<#7a7a7a>\ue0b1</> \ue625 "
				},
				{
					"background": "#2f2f2f",
					"foreground": "#6CA35E",
					"style": "diamond",
					"template": "<#7a7a7a>\ue0b1</> \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }} ",
					"type": "node"
				},
				{
					"background": "#2f2f2f",
					"foreground": "#96E072",
					"style": "diamond",
					"template": "<#7a7a7a>\ue0b1</> \ue235 ",
					"type": "python"
				},
				{
					"background": "#2f2f2f",
					"foreground": "#3891A6",
					"style": "diamond",
					"template": "<#7a7a7a>\ue0b1</> \ue77f {{ if .Unsupported }}\uf071{{ end }} ",
					"type": "dotnet"
				},
				{
					"type": "go",
					"style": "diamond",
					"backgound": "#2f2f2f",
					"foreground": "#7FD5EA",
					"template": "<#7a7a7a>\ue0b1</> \ue65e "
				},
				{
					"type": "rust",
					"style": "diamond",
					"background": "#2f2f2f",
					"foreground": "#99908a",
					"template": "<#7a7a7a>\ue0b1</> \ue68b "
				},
				{
					"type": "cmake",
					"style": "diamond",
					"foreground": "#1E9748",
					"background": "#2f2f2f",
					"template": "<#7a7a7a>\ue0b1</> \ue61d cmake "
				},
				{
					"type": "java",
					"style": "diamond",
					"foreground": "#f8981d",
					"background": "#2f2f2f",
					"template": "<#7a7a7a>\ue0b1</> \ue738 "
				},
				{
					"type": "kotlin",
					"style": "diamond",
					"foreground": "#2f2f2f",
					"background": "#906cff",
					"template": "<#7a7a7a>\ue0b1</> \ue643 "
				},
				{
					"foreground": "#FFCE5C",
					"background": "#2f2f2f",
					"properties": {
						"style": "roundrock",
						"threshold": 0
					},
					"style": "plain",
					"template": "<#7a7a7a>\ue0b1 </>{{ .Ms }}ms<#fafafa> \uf252 </>",
					"type": "executiontime"
				},
				{
					"foreground": "#9FD356",
					"background": "#2f2f2f",
					"foreground_templates": [
						"{{ if gt .Code 0 }}#E84855{{ end }}"
					],
					"properties": {
						"always_enabled": true
					},
					"style": "diamond",
					"template": "<#7a7a7a>\ue0b1</><#fafafa> \uf469 </>{{ if gt .Code 0 }}\uf467 {{ .Meaning }}{{ else }}\uf42e{{ end }} ",
					"type": "status",
					"trailing_diamond": "\u2593\ue0c6"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "#7a7a7a",
					"style": "plain",
					"template": " \u2570\u2500{{ if .Root }}#{{ else }}${{ end }}",
					"type": "text"
				}
			],
			"type": "prompt"
		}
	],
	"final_space": true,
	"secondary_prompt": {
		"background": "transparent",
		"foreground": "#7a7a7a",
		"template": " | "
	},
	"transient_prompt": {
		"background": "transparent",
		"foreground": "#7a7a7a",
		"template": " {{ .Shell }} - {{.UserName}}\u276f "
	},
	"version": 2
}