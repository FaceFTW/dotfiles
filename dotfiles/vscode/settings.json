{
	"[c]": {
		"editor.defaultFormatter": "xaver.clang-format"
	},
	"[cpp]": {
		"editor.defaultFormatter": "xaver.clang-format"
	},
	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[java]": {
		"editor.defaultFormatter": "redhat.java"
	},
	"[javascript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[markdown]": {
		"editor.defaultFormatter": "yzhang.markdown-all-in-one"
	},
	"[python]": {
		"editor.formatOnType": true
	},
	"[racket]": {
		"editor.detectIndentation": false,
		"editor.tabSize": 2
	},
	"[scss]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[sql]": {
		"editor.defaultFormatter": "mtxr.sqltools"
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[xml]": {
		"editor.defaultFormatter": "DotJoshJohnson.xml"
	},
	"[yaml]": {
		"editor.defaultFormatter": "redhat.vscode-yaml",
		"editor.detectIndentation": true,
		"editor.insertSpaces": false,
		"editor.useTabStops": false,
		"files.trimTrailingWhitespace": true
	},
	"better-comments.highlightPlainText": true,
	"better-comments.tags": [
		{
			"backgroundColor": "#FF2D00",
			"bold": false,
			"color": "#000000",
			"italic": false,
			"strikethrough": false,
			"tag": "!",
			"underline": false
		},
		{
			"backgroundColor": "#3498DB",
			"bold": false,
			"color": "#000000",
			"italic": false,
			"strikethrough": false,
			"tag": "?",
			"underline": false
		},
		{
			"backgroundColor": "transparent",
			"bold": false,
			"color": "#474747",
			"italic": false,
			"strikethrough": true,
			"tag": "//",
			"underline": false
		},
		{
			"backgroundColor": "#FF8C00",
			"bold": true,
			"color": "#000000",
			"italic": true,
			"strikethrough": false,
			"tag": "todo",
			"underline": false
		},
		{
			"backgroundColor": "#98C379",
			"bold": false,
			"color": "#000000",
			"italic": false,
			"strikethrough": false,
			"tag": "*",
			"underline": false
		},
		{
			"backgroundColor": "#FBBF24",
			"bold": true,
			"color": "#000000",
			"italic": true,
			"strikethrough": false,
			"tag": "hack",
			"underline": false
		},
		{
			"backgroundColor": "#2563EB",
			"bold": true,
			"color": "#000000",
			"italic": true,
			"strikethrough": false,
			"tag": "note",
			"underline": false
		},
		{
			"backgroundColor": "#DC2626",
			"bold": true,
			"color": "#000000",
			"italic": true,
			"strikethrough": false,
			"tag": "fix",
			"underline": false
		}
	],
	"cmake.configureOnOpen": false,
	"csharp.inlayHints.enableInlayHintsForImplicitObjectCreation": true,
	"csharp.inlayHints.enableInlayHintsForImplicitVariableTypes": true,
	"csharp.inlayHints.enableInlayHintsForLambdaParameterTypes": true,
	"csharp.semanticHighlighting.enabled": true,
	"csharp.suppressDotnetRestoreNotification": true,
	"csharp.suppressProjectJsonWarning": true,
	"debug.terminal.clearBeforeReusing": true,
	"diffEditor.codeLens": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"discord.suppressNotifications": true,
	"dotnet.completion.showCompletionItemsFromUnimportedNamespaces": true,
	"dotnet.inlayHints.enableInlayHintsForParameters": true,
	"editor.accessibilitySupport": "off",
	"editor.autoIndent": "advanced",
	"editor.detectIndentation": false,
	"editor.foldingImportsByDefault": true,
	"editor.fontFamily": "Hack NF",
	"editor.fontSize": 12,
	"editor.inlineSuggest.enabled": true,
	"editor.insertSpaces": false,
	"editor.linkedEditing": true,
	"editor.renderWhitespace": "all",
	"editor.smoothScrolling": true,
	"editor.suggest.preview": true,
	"editor.suggestSelection": "first",
	"editor.tokenColorCustomizations": {
		"[dark-plus-syntax]": {
			"comments": "#509f50"
		}
	},
	"editor.unicodeHighlight.nonBasicASCII": false,
	"editor.wrappingStrategy": "advanced",
	"eslint.alwaysShowStatus": true,
	"eslint.format.enable": true,
	"eslint.lintTask.enable": true,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"files.associations": {
		"*.meta": "ini"
	},
	"files.exclude": {
		"**/node_modules": true
	},
	"files.trimTrailingWhitespace": true,
	"git.autofetch": true,
	"git.autoStash": true,
	"git.confirmSync": false,
	"git.enableCommitSigning": true,
	"git.enableSmartCommit": true,
	"git.fetchOnPull": true,
	"git.openAfterClone": "whenNoFolderOpen",
	"git.openRepositoryInParentFolders": "always",
	"git.showPushSuccessNotification": true,
	"git.supportCancellation": true,
	"git.terminalAuthentication": false,
	"github.copilot.enable": {
		"*": true,
		"markdown": true,
		"plaintext": true,
		"scminput": false,
		"yaml": true
	},
	"githubPullRequests.createOnPublishBranch": "never",
	"githubPullRequests.pullBranch": "never",
	"githubPullRequests.queries": [
		{
			"label": "Waiting For My Review",
			"query": "is:open review-requested:${user}"
		},
		{
			"label": "Assigned To Me",
			"query": "is:open assignee:${user}"
		},
		{
			"label": "Created By Me",
			"query": "is:open author:${user}"
		}
	],
	"java.completion.guessMethodArguments": true,
	"java.debug.settings.showHex": true,
	"java.debug.settings.showQualifiedNames": false,
	"java.debug.settings.showStaticVariables": true,
	"java.eclipse.downloadSources": true,
	"java.maven.downloadSources": true,
	"javascript.format.semicolons": "insert",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"json.schemas": [
		{
			"fileMatch": [
				"*.schema.json"
			],
			"url": "http://json-schema.org/draft-07/schema"
		}
	],
	"markdown.extension.tableFormatter.normalizeIndentation": true,
	"markdown.preview.breaks": true,
	"markdown.preview.typographer": true,
	"markdownlint.config": {
		"MD001": false,
		"MD002": false,
		"MD010": false,
		"MD024": false,
		"MD025": false,
		"MD041": false
	},
	"maven.executable.path": "C:\\maven\\bin\\mvn.cmd",
	"notebook.cellToolbarLocation": {
		"default": "right",
		"polyglot-notebook": "right"
	},
	"notebook.formatOnSave.enabled": true,
	"notebook.lineNumbers": "on",
	"omnisharp.enableDecompilationSupport": true,
	"omnisharp.enableRoslynAnalyzers": true,
	"omnisharp.loggingLevel": "trace",
	"omnisharp.organizeImportsOnFormat": true,
	"omnisharp.projectFilesExcludePattern": "**/node_modules/**,**/.git/**,**/bower_components/**,**/build/**",
	"plantuml.exportFormat": "png",
	"plantuml.java": "",
	"plantuml.previewSnapIndicators": true,
	"plantuml.render": "PlantUMLServer",
	"plantuml.server": "https://www.plantuml.com/plantuml",
	"plantuml.urlFormat": "png",
	"polyglot-notebook.defaultNotebookExtension": ".dib",
	"polyglot-notebook.defaultNotebookLanguage": "csharp",
	"prettier.bracketSameLine": true,
	"prettier.bracketSpacing": false,
	"prettier.endOfLine": "crlf",
	"prettier.jsxSingleQuote": true,
	"prettier.printWidth": 100,
	"prettier.singleQuote": true,
	"prettier.tabWidth": 4,
	"prettier.trailingComma": "all",
	"prettier.useTabs": true,
	"python.languageServer": "Jedi",
	"r.alwaysUseActiveTerminal": true,
	"razor.devmode": true,
	"razor.languageServer.debug": true,
	"redhat.telemetry.enabled": true,
	"security.workspace.trust.banner": "never",
	"security.workspace.trust.untrustedFiles": "open",
	"terminal.integrated.allowChords": false,
	"terminal.integrated.altClickMovesCursor": false,
	"terminal.integrated.defaultProfile.linux": "zsh",
	"terminal.integrated.defaultProfile.windows": "PowerShell",
	"terminal.integrated.enableBell": true,
	"terminal.integrated.enableMultiLinePasteWarning": false,
	"terminal.integrated.fontFamily": "Hack NF",
	"terminal.integrated.fontSize": 12,
	"terminal.integrated.macOptionClickForcesSelection": true,
	"terminal.integrated.persistentSessionReviveProcess": "never",
	"terminal.integrated.profiles.windows": {
		"Command Prompt": {
			"args": [],
			"icon": "terminal-cmd",
			"path": [
				"${env:windir}\\Sysnative\\cmd.exe",
				"${env:windir}\\System32\\cmd.exe"
			]
		},
		"Git Bash": {
			"source": "Git Bash"
		},
		"PowerShell": {
			"icon": "terminal-powershell",
			"source": "PowerShell"
		},
		"VS 2022 Dev Prompt": {
			"args": [
				"-NoExit",
				"-Command \"&{Import-Module \"\"\"C:\\Program Files\\Microsoft Visual Studio\\2022\\Community\\Common7\\Tools\\Microsoft.VisualStudio.DevShell.dll\"\"\"; Enter-VsDevShell ad4ae33a -DevCmdArguments \"\"\"-arch=x64 -host_arch=x64\"\"\"}\""
			],
			"icon": "code",
			"source": "PowerShell"
		}
	},
	"terminal.integrated.sendKeybindingsToShell": true,
	"typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
	"typescript.format.semicolons": "insert",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"update.mode": "start",
	"vim.foldfix": true,
	"vim.handleKeys": {
		"<C-d>": true,
		"<C-f>": false,
		"<C-h>": false
	},
	"vim.highlightedyank.enable": true,
	"vim.visualstar": true,
	"window.commandCenter": true,
	"window.menuBarVisibility": "compact",
	"window.restoreWindows": "all",
	"workbench.colorCustomizations": {
		"activityBar.activeBorder": "#087f23",
		"activityBarBadge.background": "#087f23",
		"breadcrumb.activeSelectionForeground": "#087f23",
		"button.background": "#087f23",
		"checkbox.background": "#087f23",
		"editor.findMatchBorder": "#087f23",
		"editorSuggestWidget.highlightForeground": "#087f23",
		"editorWidget.border": "#087f23",
		"editorWidget.resizeBorder": "#087f23",
		"list.activeSelectionForeground": "#087f23",
		"list.highlightForeground": "#087f23",
		"list.inactiveSelectionForeground": "#087f23",
		"menubar.selectionForeground": "#087f23",
		"notificationLink.foreground": "#087f23",
		"pickerGroup.foreground": "#087f23",
		"progressBar.background": "#087f23",
		"scrollbarSlider.activeBackground": "#087f2350",
		"selection.background": "#087f2340",
		"settings.headerForeground": "#087f23",
		"settings.modifiedItemIndicator": "#087f23",
		"statusBar.background": "#087f23",
		"statusBar.noFolderBackground": "#087f23",
		"statusBarItem.remoteBackground": "#064e17",
		"tab.activeBorder": "#087f23",
	},
	"workbench.editor.enablePreview": false,
	"workbench.editor.highlightModifiedTabs": true,
	"workbench.editor.revealIfOpen": true,
	"workbench.editor.splitInGroupLayout": "vertical",
	"workbench.editor.untitled.hint": "hidden",
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter-notebook"
	},
	"workbench.layoutControl.enabled": false,
	"workbench.productIconTheme": "icons-carbon",
	"workbench.settings.useSplitJSON": true,
	"workbench.startupEditor": "none",
	"workbench.tips.enabled": false,
	"workbench.tree.indent": 16,
	"workbench.tree.renderIndentGuides": "always",
	"zenMode.hideLineNumbers": false,
	"extensions.ignoreRecommendations": true,
	"workbench.colorTheme": "Visual Studio 2019 Dark"
}