{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "secondary_prompt": {
    "template": " | ",
    "foreground": "#7a7a7a",
    "background": "transparent"
  },
  "transient_prompt": {
    "template": " {{ .Shell }} - {{.UserName}}‚ùØ ",
    "foreground": "#7a7a7a",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "cache_duration": "none",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f"
          },
          "leading_diamond": "\ue0c7\u2593",
          "template": " {{ if .WSL }}\ue712 on {{ end }}{{ .Icon }}<#7a7a7a> \ue0b1</>",
          "foreground": "#26C6DA",
          "background": "#2f2f2f",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .Name }} -",
          "foreground": "#fafafa",
          "background": "#2f2f2f",
          "type": "shell",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ if ne .Env.POSH_SESSION_DEFAULT_USER .UserName }}<#77f5d6>{{ .UserName }}</><#7a7a7a>@</>{{ end }}<#2EEFBF>{{ .HostName }}</><#7a7a7a> \ue0b1</>",
          "foreground": "#fafafa",
          "background": "#2f2f2f",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf0e7<#7a7a7a> \ue0b1</>",
          "foreground": "#ffff66",
          "background": "#2f2f2f",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "<#B5B2C2>\ue5fe </>",
            "style": "unique"
          },
          "template": "<#f2f200> \ue5ff </>{{ .Path }} ",
          "foreground": "#fafafa",
          "background": "#2f2f2f",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": "<#7a7a7a>\ue0b1</> {{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}<#E84855> \uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#2FDA4E> \uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} <#7a7a7a>\ue0b1</> ",
          "foreground": "#ffeb3b",
          "background": "#2f2f2f",
          "type": "git",
          "style": "diamond",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb3b{{ end }}",
            "{{ if gt .Ahead 0 }}#2EC4B6{{ end }}",
            "{{ if gt .Behind 0 }}#8A4FFF{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "roundrock",
            "threshold": 0
          },
          "template": "{{ .Ms }}ms<#fafafa> \uf252 </>",
          "foreground": "#FFCE5C",
          "background": "#2f2f2f",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "trailing_diamond": "\u2593\ue0c6",
          "template": "<#7a7a7a>\ue0b1</><#fafafa> \uf469 </>{{ if gt .Code 0 }}\uf467 {{ .Meaning }}{{ else }}\uf42e{{ end }} ",
          "foreground": "#9FD356",
          "background": "#2f2f2f",
          "type": "status",
          "style": "diamond",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E84855{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \u2570\u2500{{ if .Root }}#{{ else }}${{ end }}",
          "foreground": "#7a7a7a",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "shell_integration": true,
  "patch_pwsh_bleed": true,
  "final_space": true
}
